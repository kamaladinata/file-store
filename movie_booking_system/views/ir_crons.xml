<odoo>
    <data noupdate="1">
        <record model="ir.cron" id="unreserve_booking_product_qty">
            <field name="name">Unreserve Booking Product Quantities From Cart</field>
            <field name="active" eval="False"/>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="model_id" ref="model_sale_order_line"/>
            <field name="code">model.remove_booking_product_draft_order_lines()</field>
        </record>

        <record model="ir.cron" id="clean_booking_data">
            <field name="name">Clean Booking Data</field>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="model_id" ref="model_seat_book"/>
            <field name="code">model.clean_booking_data()</field>
        </record>

        <record model="ir.cron" id="remove_product_cart">
            <field name="name">Remove Product from Cart</field>
            <field name="active" eval="True"/>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
            <field name="state">code</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="model_id" ref="model_sale_order"/>
            <field name="code">model.remove_product_cart()</field>
        </record>
    </data>
</odoo>
