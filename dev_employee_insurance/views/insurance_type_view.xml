<odoo>
    <!--tree-->
	<record id="tree_employee_insurance_type" model="ir.ui.view">
		<field name="name">tree.employee.insurance.type</field>
		<field name="model">dev.employee.insurance.type</field>
		<field name="arch" type="xml">
		    <tree string="Employee Insurance Type Tree" editable="bottom">
                <field name="name"/>
 		    </tree>
		</field>
	</record>

	<!--Action-->
	<record id="action_employee_insurance_type" model="ir.actions.act_window">
        <field name="name">Insurance Type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">dev.employee.insurance.type</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="tree_employee_insurance_type"/>
    </record>

    <!--Menus-->
 	<menuitem name="Insurance Type"
              id='menu_employee_insurance_type'
              parent="dev_employee_insurance.config_menu_employee_insurance"
              action="action_employee_insurance_type"
              sequence="2"/>
</odoo>