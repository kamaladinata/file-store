<odoo>
    <report id="menu_employee_insurance_pdf_print"
            model="dev.employee.insurance"
            string="Insurance Details"
            report_type="qweb-pdf"
            name="dev_employee_insurance.employee_insurance_pdf_template"
            file="dev_employee_insurance.employee_insurance_pdf_template"
            >
    </report>

    <record id="paperformat_employee_insurance_pdf" model="report.paperformat">
        <field name="name">Employee Insurance PDF</field>
        <field name="default" eval="True" />
        <field name="format">A4</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">58</field>
        <field name="margin_bottom">15</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">53</field>
        <field name="dpi">90</field>
    </record>

    <record id="dev_employee_insurance.menu_employee_insurance_pdf_print" model="ir.actions.report">
        <field name="paperformat_id" ref="dev_employee_insurance.paperformat_employee_insurance_pdf" />
    </record>
</odoo>